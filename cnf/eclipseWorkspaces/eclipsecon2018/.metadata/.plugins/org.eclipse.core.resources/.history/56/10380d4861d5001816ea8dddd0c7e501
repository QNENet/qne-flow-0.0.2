package net.qnenet.qSiteUser.api.view;

import java.util.Map;

import org.osgi.annotation.versioning.ProviderType;



/**
 * <p>
 * This is an example of an interface that is expected to be implemented by Providers of the API. Adding methods to this
 * interface is a minor change, because only Providers will be affected.
 * </p>
 * 
 * @see ProviderType
 * @since 1.0
 */
@ProviderType
public interface QViewRegister {

	Map<String, QViewFactory> getQViews();
	

	QViewFactory getViewFactory(String viewName);


	Map<String, QViewFactory> getViewFactories();


//	QVToolbarBase getToolbarBase();


//	Map<String, QViewFactoryLocal> getViewFactoriesLocal();
//
//
//	QViewFactoryLocal getViewFactoryLocal(String viewName);

}
